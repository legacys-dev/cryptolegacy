{
  "presets": ["env", "es2015", "react"],
  "plugins": [
    ["transform-object-rest-spread", {"useBuiltIns": true}],
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "app": "./app"
        }
      }
    ]
  ]
}
